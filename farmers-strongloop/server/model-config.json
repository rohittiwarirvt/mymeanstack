{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "mongoDb"
  },
  "AccessToken": {
    "dataSource": "mongoDb",
    "public": false
  },
  "ACL": {
    "dataSource": "mongoDb",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "mongoDb",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "mongoDb",
    "public": false
  },
  "customer": {
    "dataSource": "mongoDb",
    "public": true
  },
  "subscription": {
    "dataSource": "mongoDb",
    "public": true
  },
  "plan": {
    "dataSource": "mongoDb",
    "public": true
  },
  "product": {
    "dataSource": "mongoDb",
    "public": true
  },
  "transaction": {
    "dataSource": "mongoDb",
    "public": true
  },
  "voucher": {
    "dataSource": "mongoDb",
    "public": true
  },
  "issue": {
    "dataSource": "mongoDb",
    "public": true
  }
}
